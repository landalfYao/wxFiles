view{
  font-family: '微软雅黑';
  font-size: 30rpx;
  overflow-x: hidden;
}
.search{
  padding: 15rpx;
}
.search .input{
  float: right;
  width:600rpx;
  height: 60rpx;
  background: #e2e3e5;
  border-radius: 5px;
  text-align: center
}
.search .input image{
  width: 30rpx;
  height: 30rpx;
  margin-top: 15rpx;
}
.search .input text{
 position: relative;
 top: -2rpx;
 color: #8d8e92;
}
.panel{
  clear: both;
  background: #f2f2f2;
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 15px;
  padding-right: 15px;
}
.panel .item{
  background: #ffffff;
  border-radius: 5px;
  box-shadow: 3px 3px 3px #c4c3c3;
}
.panel .item .head{
  height: 20px;
  background: #ffffff;
  border-radius: 5px 5px 0 0;
}
.panel .item .msg{
  padding: 10px;
  border-radius: 0 0 5px 5px;
}
.panel .item .msg .line{
  border-bottom: 1px solid #f2f2f2;
}
.panel .item .msg .line .header{
  width:180rpx;
  height: 180rpx;
  border-radius: 5px;
}
.panel .item .msg .line .gender{
  width:46rpx;
  height: 46rpx;
  border-radius: 32.5rpx;
  margin-left: -32.5rpx;
}
.panel .item .msg .line .dot{
  float: right;
}
.panel .item .msg .line .dot image{
  width: 35rpx;
  height: 35rpx;
  margin-top: 8rpx
}
.panel .item .msg .line .dot view{
  float: right;
  margin-top:2px;
  color: #06c1ae;
}
.panel .item .msg .line .type{
  width:80rpx;
  height:80rpx;
  margin-left: 40rpx;
}
.line-gray{
  border: 1px solid #f2f2f2; 
}
.line-msg{
  padding-top: 20rpx;
  clear: both
}
.line-msg image{
  width:45rpx;
  height:45rpx;
  float: left;
}
.line-msg .title{
  float: left;
  margin-left: 20rpx;
  margin-top: 2rpx;
}
.link{
  text-align: center;
  color: #06c1ae;
  padding-top: 20rpx;
}
.btn{
  padding: 10px;
  background: #f2f2f2
}
.btn view{
  text-align: center;
  border-radius: 5px;
  border: 1px solid #d6d5d6;
  line-height: 90rpx;
  background: #fbfafc;
  
}
.btn-theme view{
  background: #06c1ae;
  color: #ffffff;
}
.btn-theme-xs{
  width: 100rpx;
  line-height: 60rpx;
  color: #fff;
  padding-left: 10px;
  padding-right: 10px;
  background:#06c1ae;
  border-radius: 5px;
  text-align: center;
  margin-top: 20rpx;
}
.btn-default-xs{
  width: 100rpx;
  line-height: 60rpx;
  color: #999999;
  padding-left: 10px;
  padding-right: 10px;
  background:#fff;
  border-radius: 5px;
  text-align: center;
  margin-top: 20rpx;
  border: 1px solid #999999
}
.hidden{
  display: none
}
.soli{
  padding-top: 50rpx;
  text-align: center;   
}
.soli .title{
  font-size: 35rpx;
  padding-bottom: 30rpx;
}
.soli .notice{
  color: #888888;
  padding-bottom: 30rpx;
}
.soli image{
  width:250rpx;
  height: 250rpx;
}