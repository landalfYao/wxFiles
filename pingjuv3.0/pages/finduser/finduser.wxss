.header .bg{
    width:100%;
    height:340rpx;
}
.header .head{
    width:150rpx;
    height:150rpx;
    border: 6px solid #fff;
    border-radius: 150rpx; 
    position: absolute;
    left:290rpx;
    top:220rpx;
}
.header .title{
    padding-top: 50rpx;
    width: 100%;
    text-align: center;
    font-size: 35rpx;
}
.header .bar{
    margin-top: 10px;
}
.header .bar view{
    padding-top: 10px;
    width:49.5%;
    text-align: center;
    font-size: 30rpx;
    padding-bottom: 15px;
    float: left
}
.active{
    color: #06c1ae;
    border-bottom: 1px solid #06c1ae;
}
.item{
    padding: 15px;
    border-bottom: 1px #f2f2f2 solid;
}
.item .left{
    float: left;
}
.item .right{
    float: right;
    color: #888888
}
.sads{
    padding-bottom: 15px;
}
.sads .sad{
    margin-top: 20rpx;
    margin-left:20rpx;
    float: left;
    width:155rpx;
    text-align: center;
    line-height: 65rpx;
    border-radius: 30rpx;
    color:#888888;
    border: 1px solid #f2f2f2;
}
.its .item{
    padding: 10px;
    border-bottom: 1px solid #f2f2f2;
}
.its .item .gallery{
    width:50rpx;
    height: 50rpx;
    float: left;
}
.its .item .text{
    float: left;
    margin-top: 5rpx;
    margin-left: 20rpx;
}
.its .item .ic{
    float: right;
    width:40rpx;
    height: 40rpx;
    margin-top: 5rpx;
}
.its .item .li{
    margin-top: 10px;
}
.its .item .li image{
    width: 230rpx;
    height: 230rpx;
    float: left;
    margin-left: 2px;
    margin-top: 2px;
}