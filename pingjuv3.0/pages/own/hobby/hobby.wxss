.head{
    height: 210rpx;
    border-bottom: 1px solid #f2f2f2;
    text-align: center;
    padding-top: 80rpx;
}
.head .title{
    font-size: 35rpx;
}
.head .text{
    padding-top: 40rpx;
    color: #aeaeae;
}
.lit{
    background: #f2f2f2;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}
.lit .con{
    width: 30%;
    padding: 1.5%;
    line-height: 65rpx;
    float: left;
    background: #f2f2f2;
}
.lit .con view{
    color: #888888;
    text-align: center;
    border: 1px solid #888888;
    border-radius: 30rpx;
}
