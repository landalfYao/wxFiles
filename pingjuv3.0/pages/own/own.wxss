/*btn start*/
.btn{line-height: 60rpx;padding-left: 20rpx; padding-right: 20rpx; border-radius: 5px}
.btn-primary{background: #8DB6CD;color: #fff;box-shadow: 2px 2px 2px #B4CDCD;}
.btn-primary:active{background: #9FB6CD;color: #fff;box-shadow: 2px 2px 2px #B4CDCD;}
.btn-success{background: #7CCD7C;color: #fff;box-shadow: 2px 2px 2px #9BCD9B;}
.btn-success:active{background: #9BCD9B;color: #fff;box-shadow: 2px 2px 2px #9BCD9B;}
.btn-danger{background: #EE2C2C;color: #fff;box-shadow: 2px 2px 2px #EE6A50;}
.btn-danger:active{background: #EE6A50;color: #fff;box-shadow: 2px 2px 2px #EE6A50;}
.btn-warring{background: #FFA500;color: #fff;box-shadow: 2px 2px 2px #FFC125;}
.btn-warring:active{background: #FFC125;color: #fff;box-shadow: 2px 2px 2px #FFC125;}
.btn-default{background: #DEDEDE;color: #000;box-shadow: 2px 2px 2px #EBEBEB;}
.btn-default:active{background: #EBEBEB;color: #000;box-shadow: 2px 2px 2px #EBEBEB;}


.btn-lg{text-align: center;font-size: 35rpx;line-height: 80rpx;}
.btn-md{font-size: 35rpx;line-height: 80rpx;float: left;}
.btn-sm{font-size: 30rpx;float: left;}
.btn-xs{font-size: 25rpx;float: left; line-height: 50rpx}
/*btn end*/

/*model default start*/
@keyframes showModel{ 0% {background:#fff; width:92%; height:0px;} 100% { background:#fff; width:92%; height:auto;} }
.model{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden; padding: 4%; text-align: center}
.model .set{ width:92%;height:auto;background:#fff;position:relative;animation:showModel 0.1s ;border-radius: 10px; }
.model .title{ padding: 10px; border-bottom: 1px solid #f1f1f1 }
.model .body{ padding: 10px;}
.model .bottom{padding: 10px; border-top:1px solid #f1f1f1; position: relative; bottom:0}
.model .bottom .cancel{ width:49%;text-align: center;float: left; color:#CDC5BF;border-right:1px solid #f1f1f1}
.model .bottom .confirm{ width:50%;text-align: center;float: left; color:#7CCD7C}
/*model default end*/

/*model slide up start*/
@keyframes showModelUp{ 0% {background:#fff; width:100%; height:0px;bottom:-500px} 100% { background:#fff; width:100%; height:auto;bottom:0} }
.model-slide-up{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden;  text-align: center}
.model-slide-up .set{ width:100%;height:auto;background:#fff;position:absolute;bottom:0;animation:showModelUp 0.1s ;animation-fill-mode: forwards;}
.model-slide-up .body{ padding: 10px;}
.model-slide-up .top{padding: 10px; border-top:1px solid #f1f1f1;}
.model-slide-up .top .cancel{ float: left; color:#CDC5BF; font-size: 30rpx}
.model-slide-up .top .confirm{ float: right; color:#7CCD7C;font-size: 30rpx}
/*model slide up start*/

/*model slide right start*/
@keyframes showModelRight{ 0% {background:#fff; min-width:0;width:0; height:100%;} 100% { background:#fff; width:auto; min-width:400rpx; height:100%;} }
.model-slide-right{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden;  text-align: center}
.model-slide-right .set{ min-width:400rpx;height:100%;background:#fff;position:absolute;top:-251rpx;left:0;animation:showModelRight 0.1s ;animation-fill-mode: forwards;}
.model-slide-right .body{ padding: 10px;}
.model-slide-right .top{padding: 10px; border-top:1px solid #f1f1f1;}
/*model slide right start*/

/*toast start*/
@keyframes toast{
    0%   {top: 54%; background:rgba(0, 0, 0,0.5);opacity: 0}
    25%  {top: 48%; background:rgba(0, 0, 0,0.6);opacity: 1}
    50%  {top: 48%; background:rgba(0, 0, 0,0.6);opacity: 1}
    75%  {top: 40%; background:rgba(0, 0, 0,0.6);opacity: 0.2}
    100% {top: 40%;background:rgba(0, 0, 0,0.5); opacity: 0}
}
.toast{width: 50%; line-height: 70rpx; position: fixed; top: 48%;left:25%; font-size: 30rpx;background: rgba(0,0,0,0);color: #fff;animation: toast 4s;animation-fill-mode: forwards; padding-left: 20rpx; padding-right: 20rpx;border-radius:5px ;text-align: center}
@keyframes toastUp{
    0%   {top: 0; background:rgba(0, 0, 0,0.5);opacity: 0}
    25%  {top: 20px; background:rgba(0, 0, 0,0.6);opacity: 1}
    50%  {top: 20px; background:rgba(0, 0, 0,0.6);opacity: 1}
    75%  {top: 20px; background:rgba(0, 0, 0,0.6);opacity: 1}
    100% {top: 0;background:rgba(0, 0, 0,0.5); opacity: 0}
}
.toast-up{width: 50%; line-height: 70rpx; position: fixed; top: 0;left:25%; font-size: 30rpx;background: rgba(0,0,0,0);color: #fff;animation: toastUp 4s;animation-fill-mode: forwards; padding-left: 20rpx; padding-right: 20rpx;border-radius:5px ;text-align: center}
 
@keyframes toastLeftToRight{
    0%   {top: 50px; left: 0; background:rgba(0, 0, 0,0.5);opacity: 0}
    25%  {top: 50px; left: 25%;background:rgba(0, 0, 0,0.6);opacity: 1}
    50%  {top: 50px; left: 25%;background:rgba(0, 0, 0,0.6);opacity: 1}
    75%  {top: 50px; left: 25%;background:rgba(0, 0, 0,0.6);opacity: 1}
    100% {top: 50px; left: 100%;background:rgba(0, 0, 0,0.5); opacity: 0}
}
.toast-left-right{width: 50%; line-height: 70rpx; position: fixed; top: 0;left:25%; font-size: 30rpx;background: rgba(0,0,0,0);color: #fff;animation: toastLeftToRight 4s;animation-fill-mode: forwards; padding-left: 20rpx; padding-right: 20rpx;border-radius:5px ;text-align: center}
/*toast end*/