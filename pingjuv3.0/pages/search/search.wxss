.as{
    padding: 15px;
    padding-bottom: 10px;
    background: #fafafa
}
.as .area{
    float: left;
    color: #888888;
}
.as .area view{
    float: left;
}
.as .area image{
    float: left;
    width:30rpx;
    height: 30rpx;
    margin-top: 8rpx;
}
.as .ser{
    margin-left: 20rpx;
    width: 440rpx;
    float: left;
    border-bottom: 1px solid #888888;
}
.as .ser image{
    width: 35rpx;
    height: 35rpx;
    float: left;
}
.as .ser input{
    float: left;
    margin-left: 20rpx;
}
.btns{
    float: right;
    width:100rpx;
    background: #06c1ae;
    color: #fff;
    border-radius: 30rpx;
    padding:10rpx;
    text-align: center;
}

.its{
     border-bottom: 1px solid #f2f2f2;
}
.its .items{
    float: left;
    text-align: center;
    line-height: 100rpx;
    margin-left: 20rpx;
    margin-right: 15rpx;
   
}
.its .items view{
    font-size: 25rpx;
    float: left;
}
.its .items image{
    width:20rpx;
    height:20rpx;
    float: left;
    margin-top: 40rpx;
}
.ite{
    border-bottom:1px solid #f2f2f2; 
    float: left;
    width: 100%;
    padding: 10px;
}
.ite image{
    float: right;
    width:40rpx;
    height:35rpx;
    margin-top: -50rpx;
    margin-right: 50rpx;
}
.close{
    display: none;
}