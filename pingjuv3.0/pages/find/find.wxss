.header{
    padding: 15px;
    border-bottom: 1px solid #f2f2f2;
}
.header .head{
    width: 80rpx;
    height: 80rpx;
    float: left;
}
.header .gender{
    float: left;
    width: 30rpx;
    height: 30rpx;
    border-radius: 15rpx;
    margin-top: 50rpx;
    margin-left:-15rpx;
}
.header .title{
    float: left;
    margin-left: 30rpx;
    font-size: 35rpx;
    width:500rpx;
}
.header .value{
    float: left;
    margin-left: 30rpx;
    margin-top: 10rpx;
    color: #888888;
}
.header .right{
    float: right;
}
.header .right image{
    width: 35rpx;
    height: 35rpx;
    float: right;
    margin-right: 10rpx;
    margin-top: 10rpx;
}
.vela{
    padding-left: 10px;
    padding-top: 10px;
}
.its{
    padding-bottom: 10px;
    border-bottom: 25rpx #f2f2f2 solid
}
.its .it{
    float: left;
    width: 120rpx;
    line-height: 65rpx;
    background: #06c1ae;
    color: #fff;
    text-align: center;
    margin-left: 20rpx;
    margin-top:20rpx;
    border-radius: 30rpx;
}

.star{
    padding: 15px;
    font-size: 35rpx;
}
.star .title{
    float: left;
}
.stars{
    float: left;
    margin-left: 30rpx;
}
.stars image{
    float: left;
    width: 40rpx;
    height: 40rpx;
    margin-left: 10rpx;
}
.star .item {
    padding-top: 30rpx;
}
.star .item view{
    float: left;
    width:150rpx;
    margin-left: 20rpx;
    margin-top: 20rpx;
    line-height: 65rpx;
    background: #f2f2f2;
    color: #888888;
    text-align: center;
    border-radius: 30rpx;
}
.actice{
    color: #fff;
    background: #06c1ae;
}