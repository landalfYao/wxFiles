.star{
    padding: 15px;
    font-size: 35rpx;
}
.star .title{
    float: left;
}
.stars{
    float: left;
    margin-left: 30rpx;
}
.stars image{
    float: left;
    width: 40rpx;
    height: 40rpx;
    margin-left: 10rpx;
}
.star .item {
    padding-top: 30rpx;
}
.star .item view{
    float: left;
    width:150rpx;
    margin-left: 20rpx;
    margin-top: 20rpx;
    line-height: 65rpx;
    background: #f2f2f2;
    color: #888888;
    text-align: center;
    border-radius: 30rpx;
}
.actice{
    color: #fff;
    background: #06c1ae;
}