page{
  background-color: #444
}
.container{
  padding: 60rpx;
}
.linner1{
  display: flex;
  align-items: flex-end
}
.linner1 .item1{
  margin-left: 30rpx;
  display: flex;
  height: 150rpx;
  width: 100rpx;
  background: #ff9900;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 0px 15px 0 15px;
  z-index: 3;
  justify-content: center;
  align-items: center
}
.linner1 .item2{
  display: inline-block;
  height: 75rpx;
  width: 150rpx;
  background: #33acbd;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 15px 0 15px 0;
  z-index: 2
}
.linner1 .panel{
  display: flex;
  align-items: flex-start
}
.linner1 .panel .item3{
  width: 130rpx;
  text-align: center;
  height: 300rpx;
  background: #00CC66;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 20px 0 20px 0;
  z-index: 2
}
.linner1 .panel .item4{
  width: 300rpx;
  height: 450rpx;
  background: #0bcbe0;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 0 30px 0 30px;
  z-index: 5;
  text-align: center
}
.linner1 .panel .item4:active{
  background: rgba(16, 201, 221, 0.8)
}
.linner1 .item5{
  background: #33acbd;
  width: 200rpx;
  text-align: center;
  height: 350rpx;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 20px 0 20px 0;
  z-index: 1
}
.linner1 .item5:active{
  background: rgba(33, 163, 185, 0.8)
}
.linner2{
  display: flex;
  align-items: flex-start
}
.linner2 .item6{
  margin-left: 230rpx;
  background: #33acbd;
  width: 200rpx;
  height: 350rpx;
  text-align: center;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 20px 0 20px 0;
  z-index: 1
}
.linner2 .item7{
  background: #ff9900;
  width: 170rpx;
  height: 280rpx;
  box-shadow: 0px 0px 2px 2px #333 ;
  border-radius: 0 20px 0 20px;
  text-align: center;
}
.linner2 .item7:active{
  background: rgba(255, 153, 0, 0.8)
}
view{
  font-size: 34rpx
}

.item1 image{
  display: block;
  width: 70rpx;
  height: 70rpx;
}
.ima{
  width: 80rpx;
  height: 80rpx
}
