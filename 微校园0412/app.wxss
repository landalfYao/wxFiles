.payment-panel{
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  z-index: 200000;
}
.payment-panel .input-panel{
  margin: auto;
  margin-top: 50rpx;
  width: 600rpx;
  background: #fff;
  padding: 30rpx;
  padding-top: 15rpx;
}
.payment-panel .input-panel .user-tit{
  display: flex;
  justify-content: space-between;
  padding-bottom: 15rpx;
  border-bottom: 1px solid #e1e1e1
}
.payment-panel .input-panel .user-tit .con{
  display: flex;
  justify-content: flex-start;
}
.payment-panel .input-panel .user-tit .con image{
  width: 70rpx;
  height: 70rpx;
}
.payment-panel .input-panel .user-tit .con view{
  margin-left: 30rpx;
  color: #06c1ae;
  font-size: 32rpx;
  margin-top: 15rpx;
}
.payment-panel .input-panel .txt-panel{
  text-align: center;
}
.payment-panel .input-panel .txt-panel .tit{
  text-align: center;
  font-size: 32rpx;
  margin-top: 30rpx;
}
.payment-panel .input-panel .txt-panel .pri{
  text-align: center;
  font-size: 60rpx;
  margin-top: 10rpx
}
.payment-panel .input-panel .num-penel{
  margin: auto;
  margin-top: 30rpx;
  width: 570rpx;
  height: 100rpx;
  border: 1px solid #d1d1d1;
  display: flex;
  justify-content: center;
}
.payment-panel .input-panel .num-penel .item{
  border-right: 1px solid #f1f1f1;
  width: 94rpx;
}
.payment-panel .input-panel .num-penel .item .ku{
  width: 20rpx;
  height: 20rpx;
  background: #000;
  border-radius: 20rpx;
  margin-top: 40rpx;
  margin-left: 40rpx;
}
.payment-panel .input-panel .text-panel{
  text-align: center;
  margin-top: 30rpx;
  font-size: 30rpx;
}
.payment-panel .number-panel {
  display: flex;
  justify-content: center;
  flex-flow: wrap;
  position: absolute;
  bottom: 0;
  width: 100%;
  animation: show 0.5s forwards;
}
 .payment-panel .number-panel view{
  width: 33%;
  background: #fff;
  line-height: 120rpx;
  text-align: center;
  font-size: 45rpx;
  border-bottom: 1px #f1f1f1 solid;
  border-right: 1px #f1f1f1 solid
}
.payment-panel .number-panel view:active{
  background: #e1e1e1
}
.pmhidden{
  display: none
}
.payment-panel .pay-style{
  margin: auto;
  margin-top: 250rpx;
  width: 600rpx;
  background: #fff;
  padding: 30rpx;
}
.payment-panel .pay-style .tex{
  padding-bottom: 30rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: space-between
}
.payment-panel .pay-style .item{
  padding-bottom: 30rpx;
  padding-top: 30rpx;
  display: flex;
  justify-content: flex-start
}
.payment-panel .pay-style .item:active{
  background: #f3f3f3
}
.payment-panel .pay-style .item image{
  width: 70rpx;
  height: 70rpx;
}
.payment-panel .pay-style .item view{
  font-size: 30rpx;
  margin-top: 15rpx;
  margin-left: 30rpx;
}
