.dmc{
  background: #fff;
  display: flex;
  
}
.dmc .dleft{
  width: 160rpx;
  background: #f1f1f1;
  
}
.dmc .dleft .item{
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  border-bottom:1px solid #e1e1e1
}
.dmc .dright .item{
  padding: 30rpx;
  padding-left: 0rpx;
  padding-right: 0rpx;
  display: flex;
  justify-content: space-between;
  height: 120rpx
}
.dmc .dright .item .tit{
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: flex-start;
  height: 120rpx
}
.dmc .dright .item .tit image{
  width: 130rpx;
  height: 120rpx
}
.dmc .dright .item .tit .con{
  margin-top: 8rpx;
  width: 200rpx;
  height: 120rpx;
  margin-left: 30rpx;
  position: relative;
}
.dmc .dright .item .tit .con .t1{
  width: 370rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: absolute;
}
.dmc .dright .item .tit .con .t2{
  position: absolute;
  top: 50%;
  margin-top: -20rpx;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #666666;
}
.dmc .dright .item .tit .con .t3{
  font-size: 30rpx;
  font-weight: bold;
  color: #ee5353
}
.dmc .dright .item .btngroup{
  margin-top: 85rpx;
  width: 200rpx;
  display: flex;
  justify-content: flex-end;
}
.dmc .dright .item .btngroup .rel{
  width: 48rpx;
  height: 48rpx;
}

.dmc .dright .item .btngroup .adds{
  width: 48rpx;
  height: 48rpx;
}

