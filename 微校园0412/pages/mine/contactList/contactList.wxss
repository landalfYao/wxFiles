
.btn-ori{
  width: 120rpx;
  height: 120rpx;
  line-height: 110rpx;
  text-align: center;
  border-radius: 120rpx;
  background: #06c1ae;
  font-size: 90rpx;
  color: #fff;
  box-shadow: 2px 2px 2px #c1c1c1;
  position: fixed;
  right: 30rpx;
  bottom:60rpx;
}
.psd{
  padding: 30rpx;
  text-align: center;
  padding-top: 100rpx;
}
.psd image{
  width: 180rpx;
  height: 180rpx;
}
.sdsd .tit {
  display: flex;
  justify-content: space-between;
}

.sdsd .tit .t1 {
  font-size: 30rpx;
  font-weight: bold
}
.sdsd .con{
  font-size: 30rpx;
}
page {
  background: #fff;
}

view {
  box-sizing: border-box;
}

.item-box {
  width: 700rpx;
  margin: 0 auto;
  padding: 40rpx 0;
}

.items {
  width: 100%;
}

.item {
  position: relative;
  border-top: 2rpx solid #eee;
  height: 120rpx;
  line-height: 120rpx;
  overflow: hidden;
  background-color: #fff;
}

.item:last-child {
  border-bottom: 2rpx solid #eee;
}

.inner {
  position: absolute;
  top: 0;
}

.inner.txt {
  width: 626rpx;
  z-index: 5;
  padding-left: 31rpx;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.inner.txt_1 {
  background-color: #e7e7e7;
  width: 100%;
  z-index: 5;
  padding-left: 31rpx;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.inner.del {
  background-color: #fb5d6b;
  /*background-color: #e64340;*/
  width: 124rpx;
  text-align: center;
  z-index: 4;
  right: 0;
  color: #fff;
}

.item-icon {
  width: 64rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}

.thumb {
  width: 200px;
  height: 200px;
  -webkit-overflow-scrolling: touch;
  overflow: scroll;
}

.part {
  position: relative;
  border-top: 1px solid #d2d2d2;
  height: 100rpx;
  background: #fff;
  font-family: HiraginoSansGB W3;
  overflow: hidden;
}

.part_add {
  position: relative;
  height: 36rpx;
  width: 36rpx;
  top: 30rpx;
  left: 31rpx;
}

.text {
  position: relative;
  left: 60rpx;
  top: 25rpx;
}

.wxSearch {
  position: absolute;
  top: 43px;
  left: 0;
  width: 100%;
  height: 100%;
  border-top: 1px #eee solid;
  background-color: rgba(0, 0, 0, 0.2);
}

.wxSearchInner {
  background-color: #fff;
}

.wxSearch-section {
  background-color: #fff;
  padding: 5px 10px;
  border: 1px #eee solid;
}

.wxSearch-pancel {
  display: flex;
  flex-direction: row;
}

.wxSearch-input {
  flex: 8;
  background-color: #eee;
  padding: 5px 10px;
}

.wxSearch-button {
  flex: 1;
  border: none !important;
  color: cornflowerblue !important;
}

.wxSearchMindKeyItem {
  padding: 10px 5px;
  border-bottom: 1px solid #eee;
  display: flex;
  font-size: 14px;
  color: cornflowerblue;
}

.wxSearchKeyList {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.wxSearchKeyItem {
  flex: 0 0 20%;
  font-size: 14px;
  text-align: center;
  border: 1px solid #eee;
  margin: 5px;
  padding: 5px 10px;
  border-radius: 5px;
}

.shanchu {
  float: right;
  margin-right: 20rpx;
  margin-top: 25rpx;
}
