<view class="tops">
  <picker style="margin-left:30rpx" mode="date" value="{{sdate}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
    <view >
      {{startDate}}
    </view>
  </picker>
  <view style="font-size:30rpx">{{value}}</view>
  <picker style="margin-right:30rpx" mode="date" value="{{edate}}" start="2015-09-01" end="2017-09-01" bindchange="bindeDateChange">
    <view >
      {{endDate}}
    </view>
  </picker>
</view>
<view style="height:120rpx"></view>
<view class="panel flex-start item-center bottom-line">
  <view style="font-size:30rpx;">订单数</view>
  <view style="font-size:60rpx;color:#06c1ae;margin-left:60rpx">{{zong}}</view>
</view>
<view class="panel flex-start item-center bottom-line">
  <view style="font-size:30rpx;">已完成</view>
  <view style="font-size:60rpx;color:#06c1ae;margin-left:60rpx">{{ok}}</view>
</view>
<view class="panel flex-start item-center">
  <view style="font-size:30rpx;">未完成</view>
  <view style="font-size:60rpx;color:#06c1ae;margin-left:60rpx">{{on}}</view>
</view>


<view style="height:150rpx"></view>
