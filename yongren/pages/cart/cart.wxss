.select-allbar{
    padding: 10px;
    background: #f2f2f2;

}
 .btn-def{
    width:40rpx;
    height:40rpx;
    border: 1px solid #a8a8a8;
    border-radius: 20rpx;
    float: left;
}
 .btn-cli{
    width:45rpx;
    height:45rpx;
    border: 1px solid #90EE90;
    border-radius: 22.5rpx;
    float: left;
}
.select-allbar .text{
    float: left;
    color: #888888;
    margin-left: 20rpx;
    font-size: 35rpx;
}
.hidden{
    display: none
}
.list .items{
    padding: 10px;
    border-bottom: 1px solid #f2f2f2
}
.list .items .photo{
    width: 160rpx;
    height: 160rpx;
    float: left;
    margin-left: 20rpx;
}
.list .items .title{
    float: left;
    width: 350rpx;
    white-space:nowrap; 
    overflow:hidden; 
    text-overflow:ellipsis;
    margin-left: 20rpx;
}
.del{
    float: right
}
.del image{
    width: 50rpx;
    height: 50rpx;
}
.paybar{
    position: fixed;
    bottom: 0px;
    width: 100%;
    border-top: 1px solid #f2f2f2;
    background: #fff;
}
.btn-pay{
    float: right;
    width:150rpx;
    padding: 10px;
    background: rgba(255,86,112,1.00);
    text-align: center;
    color: #fff;
}