.header{
    background: #90EE90;
    padding-top: 60rpx;
    width: 100%;
    text-align: center;
    padding-bottom: 15px;
}
.header .head{
    width:150rpx;
    height:150rpx;
    border-radius: 75rpx;
    border: 1px solid #fff;
}
.header .title{
    color: #fff;
    line-height: 50rpx;
}
.ite{
    padding: 15px;
    border-bottom: 1px solid #f2f2f2;
}
.ite .title{
    float: left;
    font-size: 30rpx;
    margin-left: 30rpx;
}
.ite .ri{
    float: right;
    width: 30rpx;
    height: 30rpx;
    margin-top: 5rpx;
}
.ite .lef{
    float: left;
    width: 40rpx;
    height: 40rpx;
    margin-top: 0rpx;
}
.ol .li{
    width: 50%;
    padding-top: 15px;
    padding-bottom: 15px;
    text-align: center;
    border-bottom: #f2f2f2 solid 1px;
    float: left;
}
.ol .li image{
    width: 30rpx;
    height: 30rpx;
    float: left;
    margin-top: 5rpx;
    margin-left: 80rpx;
}
.ol .li .nk{
    float: left;
    margin-left: 10rpx;
}