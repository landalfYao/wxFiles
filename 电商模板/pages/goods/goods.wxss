

.bottom-bar{
  position: fixed;
  width: 100%;
  height: 100rpx;
  background: #fff;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  z-index: 10
}
.kang{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  border:10rpx solid #fff;
}
.title{
  padding: 20rpx
}
.incart{
  width: 35%;
  background: #ff9900;
  text-align: center;
  line-height: 100rpx;
  color: #fff
}
.buybtn{
  width: 35%;
  background: #ff4040;
  text-align: center;
  line-height: 100rpx;
  color: #fff
}
.smbtn{
  text-align: center;
  border-right: 1px solid #e1e1e1;
  width: 15%
}
.fix-panel{
  position: fixed;
  top: 0px;
  z-index: 1000
}
.guiPanel{
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
  height: 800rpx
}
.choose-panel{
  padding: 30rpx;
  height: 298rpx;
  padding-top: 0;
  overflow-y: scroll
}
.rightbtn{
  text-align: center;
  height: 100rpx;
  color: #fff;
  line-height: 100rpx;
  background: #ff4040
}

.numInput{
  width: 100rpx;
  text-align: center;
  background: #f3f3f3;
  margin: 0 30rpx;
  border-radius: 10rpx
}
.gitem{
  float: left;
  padding: 0rpx 40rpx;
  line-height: 70rpx;
  
  margin-top: 20rpx;
  margin-right: 20rpx;
  border-radius: 10rpx
}
.gitemDef{
  background: #c1c1c1;
  color: #fff;
}
.gitemAct{
  background: #ff4040;
  color: #fff;
}
@import "/wxParse/wxParse.wxss";