page{
  background: #fff
}
.panelShops{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.panelShops .items {
  overflow: hidden;
  margin-top: 8rpx
}
.panelShops .items .con{
  background: #fff;
  text-align: center;
  padding: 15rpx 15rpx 15rpx 0
}
.but{
  background: #fff;
  line-height: normal;
  padding: 30rpx 0;
  border: none;
  display: inline-block;
}
.but::after{
  border: none
}