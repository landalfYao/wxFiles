.titleBar{
  position: fixed;
  width: 100%;
  background: #fff;
  height: 70rpx;
  display: flex;
  justify-content: flex-start
}
.titleBar .item{
  width: 20%;
  text-align: center;
}
.titleBar .act{
  border-bottom: 2px solid #FF4040;
  color: #FF4040
}
.orders{
  background: #fff;
}
.orders .title{
  padding: 20rpx 30rpx
}
.goodsitem{
  background: #f3f3f3;
  padding: 20rpx;
  border-bottom: 1px solid #e1e1e1;
}
.goodsitem:last-child{
  border-bottom: none;
}
.goodspanel{
  padding: 30rpx;
  background: #fff
}
.white{
  background: #fff
}
.buybtn{
  line-height: 70rpx;
  background: #ff9900;
  padding: 0 30rpx;
  color: #fff;
  border-radius: 5px
}