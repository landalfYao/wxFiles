view{
  font-size: 30rpx
}
.panel-center{
  display: flex;
  justify-content: center;
  flex-wrap: wrap
}
.item-center{
  align-items: center
}
.fixed-top{
  width: 100%;
  position: fixed;
  top: 0
}
.panel{
  padding: 30rpx;
}
.bottom-line{
  border-bottom: 1px solid #e1e1e1
}
.shadow-line{
  box-shadow: 1px 1px 2px 1px #e1e1e1
}
.panel-between{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.panel-start{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap
}
.bold{
  font-weight: bold
}
.font-title{
  font-size: 34rpx
}
.bg-white{
  background: #ffffff
}
.bg-gray{
  background: #f3f3f3
}
.font-def{
  color: #888888
}
.font-pri{
  color: #FF3333;
}
.font-warring{
  color: #FF6600
}
.ell{
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}
.panel-min{
  padding: 20rpx 30rpx
}
.panel-end{
  display: flex;
  justify-content: flex-end;
  flex-wrap: wrap
}
.panel-around{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap
}
.fixed-bottom{
  width: 100%;
  position: fixed;
  bottom: 0
}
.btn-theme{
  line-height: 120rpx;
  text-align: center;
  width: 100%;
  background: #0099FF;
  color: #ffffff;
  font-size: 32rpx
}
.btn-theme:active{
  background: #66CCFF
}
.bg-white{
  color: #ffffff
}
.font-22{
  font-size: 24rpx
}
.tag-success{
  color: #33CC66;
  background: #cdffde;
  font-size: 24rpx;
  line-height: 45rpx;
  padding:0 20rpx;
  text-align: center;
  border-radius: 5rpx
}
.tag-warring{
  color: #FF6600;
  background: #ffd0b1;
  font-size: 24rpx;
  line-height: 45rpx;
  padding:0 20rpx;
  text-align: center;
  border-radius: 5rpx
}
.tag-default{
  color: #888888;
  background: #e1e1e1;
  font-size: 24rpx;
  line-height: 45rpx;
  padding:0 20rpx;
  text-align: center;
  border-radius: 5rpx
}