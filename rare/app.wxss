.spinner-cube-grid {
	width: 30px;
	height: 30px;
	margin: auto;/*
   * Spinner positions
   * 1 2 3
   * 4 5 6
   * 7 8 9
   */
}
.spinner-cube-grid .sk-cube {
	width: 33.33%;
	height: 33.33%;
	background-color: #fff;
	float: left;
	-webkit-animation: sk-cubeGridScaleDelay 1.3s infinite ease-in-out;
	animation: sk-cubeGridScaleDelay 1.3s infinite ease-in-out;
}
.spinner-cube-grid.sk-cube1 {
	-webkit-animation-delay: 0.2s;
	animation-delay: 0.2s;
}
.spinner-cube-grid .sk-cube2 {
	-webkit-animation-delay: 0.3s;
	animation-delay: 0.3s;
}
.spinner-cube-grid .sk-cube3 {
	-webkit-animation-delay: 0.4s;
	animation-delay: 0.4s;
}
.spinner-cube-grid .sk-cube4 {
	-webkit-animation-delay: 0.1s;
	animation-delay: 0.1s;
}
.spinner-cube-grid .sk-cube5 {
	-webkit-animation-delay: 0.2s;
	animation-delay: 0.2s;
}
.spinner-cube-grid .sk-cube6 {
	-webkit-animation-delay: 0.3s;
	animation-delay: 0.3s;
}
.spinner-cube-grid .sk-cube7 {
	-webkit-animation-delay: 0.0s;
	animation-delay: 0.0s;
}
.spinner-cube-grid .sk-cube8 {
	-webkit-animation-delay: 0.1s;
	animation-delay: 0.1s;
}
.spinner-cube-grid .sk-cube9 {
	-webkit-animation-delay: 0.2s;
	animation-delay: 0.2s;
}
 @-webkit-keyframes sk-cubeGridScaleDelay {
 0%, 70%, 100% {
 -webkit-transform: scale3D(1, 1, 1);
 transform: scale3D(1, 1, 1);
}
 35% {
 -webkit-transform: scale3D(0, 0, 1);
 transform: scale3D(0, 0, 1);
}
}
 @keyframes sk-cubeGridScaleDelay {
 0%, 70%, 100% {
 -webkit-transform: scale3D(1, 1, 1);
 transform: scale3D(1, 1, 1);
}
 35% {
 -webkit-transform: scale3D(0, 0, 1);
 transform: scale3D(0, 0, 1);
}
}

/*model default start*/
@keyframes showModel{ 0% {background:#fff; width:92%; height:0px;} 100% { background:#fff; width:92%; height:auto;} }
.model{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden; padding: 4%; text-align: center}
.model .set{ width:92%;height:auto;background:#fff;position:relative;animation:showModel 0.1s ;border-radius: 10px; }
.model .title{ padding: 10px; border-bottom: 1px solid #f1f1f1 }
.model .body{ padding: 10px;}
.model .bottom{padding: 10px; border-top:1px solid #f1f1f1; position: relative; bottom:0}
.model .bottom .cancel{ width:49%;text-align: center;float: left; color:#CDC5BF;border-right:1px solid #f1f1f1}
.model .bottom .confirm{ width:50%;text-align: center;float: left; color:#7CCD7C}
/*model default end*/

/*model slide up start*/
@keyframes showModelUp{ 0% {background:#fff; width:100%; height:0px;bottom:-500px} 100% { background:#fff; width:100%; height:auto;bottom:0} }
.model-slide-up{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden;  text-align: center}
.model-slide-up .set{ width:100%;height:auto;background:#fff;position:absolute;bottom:0;animation:showModelUp 0.1s ;animation-fill-mode: forwards;}
.model-slide-up .body{ padding: 10px;}
.model-slide-up .top{padding: 10px; border-top:1px solid #f1f1f1;}
.model-slide-up .top .cancel{ float: left; color:#CDC5BF; font-size: 30rpx}
.model-slide-up .top .confirm{ float: right; color:#7CCD7C;font-size: 30rpx}
/*model slide up start*/

/*model slide right start*/
@keyframes showModelRight{ 0% {background:#fff; min-width:0;width:0; height:100%;} 100% { background:#fff; width:auto; min-width:400rpx; height:100%;} }
.model-slide-right{ position: fixed; top: 0;left: 0; background: rgba(0,0,0,0.4) ;overflow-y:hidden;  text-align: center}
.model-slide-right .set{ min-width:400rpx;height:100%;background:#fff;position:absolute;top:-251rpx;left:0;animation:showModelRight 0.1s ;animation-fill-mode: forwards;}
.model-slide-right .body{ padding: 10px;}
.model-slide-right .top{padding: 10px; border-top:1px solid #f1f1f1;}
/*model slide right start*/