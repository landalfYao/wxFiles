.container-full{
  background: #f3f3f3;
  overflow-y: scroll;
}
.search{
  padding: 30rpx;
  text-align: center;
  
}
.search .do{
  border-radius: 5px;
  background: #fff;
  width: 690rpx;
  box-shadow: 2px 2px 2px #cfcfcf
}
.search .se{
  width: 40rpx;
  height: 40rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
}
.search .se2{
  width: 40rpx;
  height: 40rpx;
  margin-top: 15rpx;
  margin-right: 10rpx;
}
.search .do input{
  line-height: 70rpx;
  height: 70rpx;
  width: 580rpx;
}
.search .do .li{
  float: left;
}
.classify{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.classify .item{
  width: 48%;
  background: #fff;
  box-shadow: 2px 2px 2px #cfcfcf;
  margin-top: 30rpx;
  border-radius: 5px;
}
.classify .item image{
  width: 100%;
  height: 300rpx;
}
.classify .item view{
  width: 100%;
  line-height: 100rpx;
  text-align: center
}
.titlebar{
  padding: 30rpx;
  background: #e6bfae;
  position: fixed;
  top: 0;
  width: 93%
}
.titlebar .left{
  width: 46rpx;
  height: 46rpx;
  float: left;
}
.titlebar .title{
  color: #fff;
  float: left;
}
.titlebar .sear{
  width: 46rpx;
  height: 46rpx;
  float: right;
}
.titlebar input{
  width: 150rpx;
  float: right;
  text-align: right;
  margin-right: 10rpx;
  color: #fff;
  border-bottom: 1px solid #fff;
}
