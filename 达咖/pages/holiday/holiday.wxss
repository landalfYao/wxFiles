page{
  background: #f3f3f3
}
.searchsd{
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  background: #fff
}
.searchsd input{
  width: 520rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  text-align: center;
  background: #f3f3f3;
  border-radius: 5px
}
.searchsd .searchBtn{
  width: 130rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  border-radius: 5px;
  font-size: 30rpx;
}

.gpanel{
  border-radius: 10rpx;
  box-shadow: 2px 2px 2px #c1c1c1;
}
.xpanel{
  border-radius: 10rpx;
  padding: 20rpx;
  background: rgba(255, 255, 255, 0.8);
  display: flex;
  justify-content: flex-start;
  align-items: center
}
.xpanel .content{
  margin-left: 20rpx;
  width: 400rpx;
}