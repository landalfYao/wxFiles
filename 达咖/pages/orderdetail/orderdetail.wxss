page{
  background-color: #f3f3f3
}
.back{
  height: 300rpx;
  background: #4d5d6c
}
.back .item{
  width: 50%;
  text-align: center
}
.back .item:first-child{
  margin-top: 50rpx;
}
.back .item .forwidth{
  width: 200rpx;
  margin: auto;
  margin-top: 10rpx;
}
.back .item .imgs{
  margin-top: 30rpx;
  width: 250rpx;
  height: 180rpx;
  border-radius: 5px;
  border: 2px solid rgba(255, 255, 255, 0.6)
}
.dan{
  padding-bottom: 30rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.dan .item{
  width: 25%;
  text-align: center
}
.dan .item .cri{
  margin: auto;
  width: 40rpx;
  height: 40rpx;
  margin-top: -19rpx;
  border-radius:40rpx; 
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center
}
.dan .item .cri view{
  width: 30rpx;
  height: 30rpx;
  background: #ff9900;
  border-radius:40rpx;
  margin: auto; 
}
.wm-panel{
  background: #fff;
}
.titbar{
  padding: 20rpx 30rpx;
}
.titbar .titimg{
  width: 60rpx;
  height: 50rpx;
  border-radius: 5rpx
}
.conbar{
  padding: 20rpx 30rpx
}
.conbar .item{
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  border-bottom:1px solid #e1e1e1;
  background:  #f1f1f1;
}
.conbar .item:last-child{
  border-bottom:none;
}
.conbar .item image{
  width: 140rpx;
  height: 140rpx
}
.conbar .item .t1{
  width: 400rpx
}
.conbar .item .content{
  text-align: right
}
.contxt{
  line-height: 50rpx;
}
.cantact{
  display: flex;
  justify-content: center;
}
.cantact button{
  margin-top: 20rpx;
  width: 45%;
  margin-bottom: 20rpx;
}
.cantact .item{
  padding: 20rpx 0;
  display: flex;
  justify-content: center;
  align-items: center
}
.pfix{
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: flex-end;
  padding: 20rpx 0
}
.pbtn{
  line-height: 60rpx;
  min-width: 150rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  color: #fff;
  border-radius: 60rpx;
  text-align: center;
  margin-right: 20rpx;
}