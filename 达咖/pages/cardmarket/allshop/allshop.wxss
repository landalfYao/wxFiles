.shop-list{
  padding: 20rpx 0;
}
.shop-img{
  width: 250rpx;
  height: 180rpx;
}
.shop-panel{
  width: 410rpx;
}
.shop-con .item{
  width: 40rpx;
  height: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  margin-left: 15rpx;
}
.shop-con .item:first-child{
  margin-left: 0
}
.itticon{
  width: 50%;
  height: 240rpx;
}
.itticon image{
  width: 90%;
  height: 200rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  text-align: center;
  border-radius: 10rpx;
  filter: blur(2px)
}