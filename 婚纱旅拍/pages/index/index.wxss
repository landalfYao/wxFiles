.swiper-container{
 position: relative;
}
.swiper-container swiper{
 height: 351rpx;
}
.swiper-container .swiper .img{
 width: 100%;
 height: 100%;
}
.swiper-container .dots{
 position: absolute;
 left: 0;
 right: 0;
 bottom: 30rpx;
 display: flex;
 justify-content: center;

}
.swiper-container .dots .dot{
 margin: 0 8rpx;
 width: 14rpx;
 height: 14rpx;
 background:#FFF;
 border-radius: 8rpx;
 transition: all .6s;
  margin-right: 39rpx;
}
.swiper-container .dots .dot.active{
 background:#ff7a92;
}
/*end swiper-container*/


