<!--pages/Questions/Questions.wxml-->
<scroll-view class="container" scroll-x="false">

<view class="Title">
<text>常见问题</text>
</view>
<!--end Title-->

<view class="QuestionList">

<block wx:for="{{Questions}}">

<view class="Questions" id="{{index}}" bindtap="gotoQuestionText">
<text>{{item.title}}</text>
<image src="/images/moreImg.jpg"></image>
</view>
<!--end Questions-->

</block>

</view>
<!--end QuestionList-->



</scroll-view>
