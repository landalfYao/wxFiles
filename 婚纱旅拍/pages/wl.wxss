/* pages/wl.wxss */
view{
    font-family: '微软雅黑'
}
.header{
    padding: 10px;
    background: #f79a3d;
}
.header image {
    width:60px;
    height:60px;
    float: left;
}
.header .title{
    color: #fff;
    width:  12rem;
    float: left;
    margin-left: 10px;
    white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
    font-style:  initial;
    font-size: 16px;
}
.header .ttt{
    color: #fff;
    width:  12rem;
    float: left;
    font-size: 14px;
    margin-top: 10px;
    margin-left: 10px;
}
.list{
    clear: both;
    padding-left: 20px;
    padding-right: 20px;
    padding-top:10px; 
}
.list .left{
    width: 60px;
    text-align: center;
    float: left;

}
.list .left .time{
    font-size: 10px;
    color: #787878
}
.list .right{
    float: left;
    margin-left: 20px;
    width:16em;
    font-size: 14px;
}
.lin{
    color: #787878;
    clear: both;
    margin-left: 50px;
    margin-top: 10px;
}