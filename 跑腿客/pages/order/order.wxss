.top-fixed{
  position: fixed;
  width: 100%;
}
.top-fixed .item{
  width: 33.33%;
  display: inline-block;
  text-align: center;
  background: #fff
}
.top-fixed .item view{
  line-height: 90rpx;
  background: #ffffff;
  display: inherit
}
.top-fixed .item .active{
  color: #3399cc;
  border-bottom: 2px solid #3399cc;
}
.zhong-item {
