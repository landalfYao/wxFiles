.tab_bar{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0
}
.tab_bar .tab_bar_item{
  text-align: center;
  display: inline-block
}

.container{
  height: 100vh
}


.icon-10{ width: 10rpx; height: 10rpx; }
.icon-20{ width: 20rpx; height: 20rpx; }
.icon-30{ width: 30rpx; height: 30rpx; }
.icon-40{ width: 40rpx; height: 40rpx; }
.icon-50{ width: 50rpx; height: 50rpx; }
.icon-60{ width: 60rpx; height: 60rpx; }
.icon-70{ width: 70rpx; height: 70rpx; }
.icon-80{ width: 80rpx; height: 80rpx; }
.icon-90{ width: 90rpx; height: 90rpx; }
.icon-100{ width: 100rpx; height: 100rpx; }

.ell-1{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.ell-2{
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
}
.ell-3{
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 3;
overflow: hidden;
}

.flex{
  display: flex
}
.align-items-center{
  align-items: center;
}
.justify-content-center{
  justify-content: center;
}
.justify-content-start{
  justify-content: flex-start
}
.justify-content-between{
  justify-content: space-between
}
.warp{
  flex-wrap: wrap
}

.inline-block{
  display: inline-block
}
.bg-gradient-black{
  background:-webkit-gradient(linear, 0 0, 0 bottom, from(rgba(0, 0, 0, 0)), to(rgba(0, 0, 0, 1)));  
}