<swiper indicator-dots="true" autoplay="true" interval="5000" duration="500" indicator-color="rgba(255,255,255,0.3)" indicator-active-color="#ffffff" style='width:100%;height:350rpx;overflow:hidden;'>
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" style='width:100%;height:350rpx;' mode='aspectFill' />
      </swiper-item>
    </block>
</swiper>

<view class='panel panel-between item-center'>
  <view style='width:480rpx;'>
    <view style='font-size:34rpx;font-weight:bold'>熊猫园</view>
    <view style='font-size:26rpx;color:#888888;'>营业时间：9:00~16:00</view>
  </view>
  <view class='panel-end item-center' style='width:160rpx;'>
    <image src='/img/lo.png' style='width:60rpx;height:60rpx;margin-right:30rpx'></image>
    <image src='/img/zf.png' style='width:60rpx;height:60rpx'></image>
  </view>
</view>